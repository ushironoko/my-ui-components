    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^26.0.0",
    "eslint": "^7.27.0",
